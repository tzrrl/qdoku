source "https://rubygems.org"

# Dieses Gem ist Jekyll selbst.
gem "jekyll"

# Das ist das Theme, das du verwenden möchtest.
gem "just-the-docs"

# Dies ist ein wichtiges Hilfsprogramm, das die Ausführung von Jekyll
# und seinen Abhängigkeiten steuert.
gem "bundler"

gem "jekyll-feed"
gem "jekyll-seo-tag"
gem "jekyll-sitemap"